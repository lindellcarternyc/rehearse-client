{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "jsx-boolean-value": [ false ], 
    "semicolon": [ true, "never" ],
    "no-consecutive-blank-lines": true
  }
}
